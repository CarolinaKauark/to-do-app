    "http-status-codes": "^2.2.0",
    "jest": "27.4.3",
    "jsonwebtoken": "8.5.1",
    "md5": "2.3.0",
    "mysql2": "2.3.3",
    "sequelize": "6.9.0"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "^8.33.0",
    "mocha": "9.2.1",
    "nodemon": "2.0.15",
